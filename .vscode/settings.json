{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[django-html]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "batisteo.vscode-django"
  },
  "files.associations": {
    "*.html": "django-html"
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  }
}
